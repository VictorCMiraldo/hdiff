// Input:
//   k is the accuracy of the test
//   n = 2^r * d is the number to be tested
//
// Output:
//   the value of variable result
result := 1;
while (k > 0 or result == 0) do {
  a := range 2 (n - 2);
  x := (a ^ d) % n;
  if (x == 1 or x == n - 1)
  then skip;
  else while r > 0 do {
    x := (x ^ 2) % n;
    if not (x == n - 1)
    then result := 0;
    else skip;
  }
}
